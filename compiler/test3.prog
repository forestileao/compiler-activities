func fibonacci(int n) -> int
    if n <= 1 then
        return n;
    else
        int fib1;
        int fib2;
        fib1 = fibonacci(n - 1);
        fib2 = fibonacci(n - 2);
        return fib1 + fib2;
    end
end

func main_program() -> int
    int num;
    int result;

    write("Digite um numero para calcular Fibonacci: ");
    read(num);

    if num < 0 then
        write("Numero deve ser positivo!");
        return 0;
    end

    result = fibonacci(num);

    write("Fibonacci de ");
    write(num);
    write(" eh: ");
    write(result);

    return 0;
end

int n;
int fib_result;

n = 10;
write("Calculando Fibonacci de 10...");

fib_result = fibonacci(n);

write("Resultado: ");
write(fib_result);

write("Testando alguns valores:");

n = 0;
write("Fibonacci(0) = ");
